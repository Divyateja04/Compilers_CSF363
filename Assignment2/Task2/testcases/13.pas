program AllTypes;
var
  i, j : Integer;
  a, b : Real;
  c : Char;
  d : Boolean;
  e : String;
begin
  i := 0;
  j := -5;
  a := 1.0;
  b := -5.0;
  c := 'A';
  d := True;
  e := 'Hello, World!';
  
  writeln('Integer i: ', i);
  writeln('Integer j: ', j);
  writeln('Real a: ', a);
  writeln('Real b: ', b);
  writeln('Char c: ', c);
  writeln('Boolean d: ', d);
  writeln('String e: ', e);
end.